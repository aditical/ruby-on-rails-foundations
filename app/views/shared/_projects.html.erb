<% @projects.each do |project| %>
    <p> <%#= project.inspect %>
    <p><strong><%= project.title %></strong> - <em><%= project.description %></em>
        <%#= "| #{project.percent_complete}" unless !project.percent_complete %>
        <%= "| #{project.percent_complete}" if project.percent_complete %>
    </p>
<% end %>